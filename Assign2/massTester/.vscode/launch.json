            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/mnt/d/code/cs201/assign2/massTester/working/assign",
            //"program": "/mnt/d/code/cs201/assign2/massTester/working/assign",
